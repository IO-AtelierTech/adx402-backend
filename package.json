{
  "name": "adx402-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tsoa spec-and-routes\" \"nodemon\"",
    "build": "tsoa spec-and-routes && tsc",
    "tsoa:routes": "tsoa routes && tsoa spec",
    "start": "node build/server.js",
    "start:prod": "NODE_ENV=production node build/server.js",
    "migrate:up": "drizzle-kit migrate",
    "migrate:generate": "drizzle-kit generate",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write --list-different ."
  },
  "dependencies": {
    "@tsoa/runtime": "^6.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.11.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "x402-express": "^0.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tsoa/cli": "^6.6.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/js-yaml": "^4",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.11.0"
}
